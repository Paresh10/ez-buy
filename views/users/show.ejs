  <%- include('../products/nav.ejs') %>

  <h3>All products displayed by seller: <%= user.username %></h3>
  <ul>
    <% products.forEach((product) => { %>
      <li>
        <a href="/products/<%= product.id %>"><%= product.title %></a>
      </li>
      <li>
        <p> <%= product.description %> </p>
      </li>
      <li>
        <img class="productImg"src="/<%=product.productImage%>"></p>
      </li>
    <% }) %>
  </ul>

